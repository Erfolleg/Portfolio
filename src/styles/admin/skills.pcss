.skills {
  width: 100%;

  &-cell {
    font-size: 16px;
    font-weight: 400;
    line-height: 32px;
    padding-bottom: 5px;
    &:first-child {
      width: 60%;
    }
  }

  &__input {
    display: flex;
    justify-content: space-between;
    margin-right: 50px;
    &::after {
      content: "%";
    }
  }
}
