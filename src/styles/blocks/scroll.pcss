.scroll {
  position: absolute;
  bottom: 7%;
  display: flex;
  border: none;
  background: transparent;
  cursor: pointer;
}

.scroll-down {
    padding: 5px 22px 0 0;
    
}

.scroll-down_icon {
  width: 30px;
  height: 30px;
  background: svg-load("scroll.svg", fill=#414c63, width=100%, height=100%);
}
